{
  "version": 1,
  "paths": {
    "/assets": {
      "name": "/assets",
      "lambdaFunction": "bridgeassetfunction",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/users": {
      "name": "/users",
      "lambdaFunction": "bridgeassetusers",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    }
  }
}